#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/E:/web开发/项目/vue+express+mysql/serve/node_modules/.store/gravatar@1.8.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/E:/web开发/项目/vue+express+mysql/serve/node_modules/.store/gravatar@1.8.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/gravatar@1.8.2/node_modules/gravatar/cli.js" "$@"
else
  exec node  "$basedir/../.store/gravatar@1.8.2/node_modules/gravatar/cli.js" "$@"
fi
